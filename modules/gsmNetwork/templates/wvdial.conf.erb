[Dialer Defaults]
Init1 = ATZ
Init2 = AT+CPIN?
ISDN = 0
Modem = /dev/gsmmodem
Baud = 460800
New PPPD = yes
Stupid Mode = 1

[Dialer pin]
Init2 = AT+CPIN="<%= @pin %>"

[Dialer verbose]
Init3 = ATQ0 V1 E1 +FCLASS=0

[Dialer free]
Init4 = AT+CGDCONT=1,"IP","free"
Phone = *99#
Username = free
Password = free

